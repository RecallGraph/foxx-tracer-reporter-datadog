{
  "compilerOptions": {
    "moduleResolution": "node",
    "declaration": true,
    "declarationMap": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "allowUnreachableCode": false,
    "module": "commonjs",
    "target": "es2019",
    "sourceMap": true,
    "outDir": "dist"
  },
  "exclude": [
    "node_modules"
  ]
}